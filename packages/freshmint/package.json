{
  "name": "freshmint",
  "version": "0.0.32-alpha",
  "description": "The minty fresh way to launch an NFT project.",
  "contributors": [
    "Mackenzie Kieran",
    "Peter Siemens <peterjsiemens@gmail.com>"
  ],
  "license": "Apache-2.0",
  "repository": "github:packagelabs/freshmint",
  "keywords": [
    "nft",
    "blockchain"
  ],
  "engines": {
    "node": ">=14.0.0"
  },
  "bin": {
    "fresh": "dist/index.js"
  },
  "scripts": {
    "test": "jest --runInBand --bail",
    "build": "tsup index.ts --clean --minify --format esm,cjs",
    "postbuild": "cpx --include-empty-dirs \"templates/**\" dist/templates",
    "dev": "npm run build -- --watch --onSuccess \"npm run postbuild\"",
    "lint": "eslint . --ext .ts"
  },
  "dependencies": {
    "@freshmint/core": "*",
    "@onflow/decode": "0.0.11",
    "@onflow/types": "^1.0.3",
    "chalk": "^4.1.0",
    "commander": "^7.1.0",
    "csv-parse": "^4.16.3",
    "csv-writer": "^1.6.0",
    "dotenv": "^16.0.1",
    "elliptic": "^6.5.4",
    "fast-csv": "^4.3.6",
    "fs-extra": "^10.0.0",
    "handlebars": "^4.7.7",
    "inquirer": "^8.2.2",
    "js-yaml": "^4.1.0",
    "nft.storage": "^7.0.0",
    "node-fetch": "^2.6.7",
    "ora": "^5.4.1",
    "pouchdb": "^7.2.2",
    "pouchdb-find": "^7.2.2",
    "progress": "^2.0.3"
  },
  "devDependencies": {
    "@types/elliptic": "^6.4.14",
    "@types/fs-extra": "^9.0.13",
    "@types/inquirer": "^8.2.1",
    "@types/jest": "^28.1.6",
    "@types/js-yaml": "^4.0.5",
    "@types/node-fetch": "^2.6.2",
    "@types/pouchdb": "^6.4.0",
    "@types/progress": "^2.0.5",
    "cpx": "^1.5.0",
    "jest": "^28.1.3",
    "ts-jest": "^28.0.7",
    "tsup": "^6.2.3",
    "typescript": "^4.7.4"
  }
}
