            pre {
                BlindEditionNFT_Test.editionMembers[id] == nil : \\"NFT has already been revealed\\"
            }

            let editionMember = EditionMember(
                editionID: editionID,
                serialNumber: serialNumber,
                salt: salt,
            )

            BlindEditionNFT_Test.editionMembers[id] = editionMember

            emit Revealed(
                id: id,
                editionID: editionID,
                serialNumber: serialNumber,
                salt: salt
            )
